{
  "name": "babylon-catalog-ui",
  "version": "0.0.2",
  "description": "An open source build scaffolding utility for web apps.",
  "repository": "https://github.com/redhat-cop/babylon.git",
  "homepage": "https://github.com/redhat-cop/babylon",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "npm run clean",
    "dr:surge": "node dr-surge.js",
    "build": "webpack --config webpack.prod.js && npm run dr:surge",
    "start": "sirv dist --cors --single --host --port 8080",
    "start:dev": "webpack serve --hot --color --progress --config webpack.dev.js",
    "test": "jest",
    "eslint": "eslint --ext .tsx,.js ./src/",
    "lint": "npm run eslint",
    "format": "prettier --check --write ./src/**/*.{tsx,ts}",
    "type-check": "tsc --noEmit",
    "ci-checks": "npm run type-check && npm run lint && npm run test",
    "build:bundle-profile": "webpack --config webpack.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "npm run build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook"
  },
  "devDependencies": {
    "@axe-core/react": "^4.2.2",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/addons": "^6.3.8",
    "@storybook/react": "^6.3.8",
    "@types/enzyme": "^3.10.9",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.24",
    "@types/react-router-dom": "^5.1.8",
    "@types/victory": "^33.1.5",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.2.7",
    "dotenv-webpack": "^6.0.4",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.2",
    "imagemin": "^7.0.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.6.2",
    "optimize-css-assets-webpack-plugin": "^5.0.8",
    "prettier": "^2.4.0",
    "prop-types": "^15.6.1",
    "raw-loader": "^4.0.2",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-hot-loader": "^4.13.0",
    "react-router-dom": "^5.3.0",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^7.1.1",
    "terser-webpack-plugin": "^5.2.4",
    "ts-jest": "^26.5.6",
    "ts-loader": "^8.3.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.52.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^3.11.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^17.0.1",
    "@monaco-editor/react": "^4.2.2",
    "@patternfly/patternfly": "^4.132.2",
    "@patternfly/react-core": "^4.152.4",
    "@patternfly/react-icons": "^4.11.14",
    "@patternfly/react-styles": "^4.11.13",
    "@patternfly/react-table": "^4.29.58",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@reduxjs/toolkit": "^1.6.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@types/react-redux": "^7.1.18",
    "asciidoctor": "^2.2.5",
    "classnames": "^2.3.1",
    "crypto-browserify": "3.12.0",
    "dompurify": "^2.3.1",
    "js-yaml": "^3.14.1",
    "parse-duration": "^1.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "react-router-last-location": "^2.0.1",
    "react-star-ratings": "^2.3.0",
    "redux": "^4.1.1",
    "reselect": "^4.0.0",
    "sirv-cli": "^1.0.14",
    "stream-browserify": "3.0.0"
  },
  "browser": {
    "crypto": false
  }
}
