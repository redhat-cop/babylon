{
  "name": "babylon-catalog-ui",
  "version": "0.0.2",
  "description": "An open source build scaffolding utility for web apps.",
  "repository": "https://github.com/redhat-cop/babylon.git",
  "homepage": "https://github.com/redhat-cop/babylon",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "if test \"$KEEP_ASSETS\" != \"true\" ; then npm run clean ; fi ",
    "dr:surge": "node dr-surge.js",
    "build": "webpack --config webpack.prod.js && npm run dr:surge",
    "start": "sirv dist --cors --single --host --port 8080",
    "start:dev": "webpack serve --color --progress --config webpack.dev.js",
    "test": "jest",
    "eslint": "eslint --ext .tsx,.js ./src/",
    "lint": "npm run eslint",
    "format": "prettier --check --write \"./**/*.{tsx,ts,json,css,js}\"",
    "type-check": "tsc --noEmit",
    "ci-checks": "npm run type-check && npm run lint && npm run test",
    "build:bundle-profile": "webpack --config webpack.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "npm run build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.2.0",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/json2csv": "^5.0.3",
    "@types/lodash.throttle": "^4.1.9",
    "@types/react": "^18.3.13",
    "@types/react-dom": "^18.3.1",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^9.2.1",
    "babel-plugin-transform-imports": "^2.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.11.0",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "dotenv-webpack": "^8.1.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.4",
    "file-loader": "^6.2.0",
    "history": "^5.3.0",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.5.2",
    "postcss-custom-media": "^8.0.2",
    "postcss-loader": "^7.3.4",
    "prettier": "^3.5.1",
    "prop-types": "^15.6.1",
    "raw-loader": "^4.0.2",
    "rimraf": "^6.0.1",
    "sirv-cli": "^3.0.1",
    "style-loader": "^3.3.4",
    "svg-url-loader": "^8.0.0",
    "terser-webpack-plugin": "^5.3.11",
    "ts-jest": "^29.2.5",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.98.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.0",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "@lexical/react": "^0.11.1",
    "@monaco-editor/react": "^4.7.0",
    "@patternfly/react-core": "^5.3.3",
    "@patternfly/react-icons": "^5.3.2",
    "@patternfly/react-styles": "^5.3.1",
    "@patternfly/react-table": "^5.3.3",
    "@reduxjs/toolkit": "^1.9.7",
    "asciidoctor": "^3.0.2",
    "dompurify": "^3.2.4",
    "fuse.js": "^7.1.0",
    "js-yaml": "^4.1.0",
    "json2csv": "^5.0.7",
    "lexical": "^0.11.1",
    "lodash.throttle": "^4.1.1",
    "moment": "^2.30.1",
    "parse-duration": "^1.1.1",
    "react": "^18.2.0",
    "react-big-calendar": "^1.17.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^3.1.4",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.28.0",
    "react-window": "^1.8.11",
    "redux": "^4.2.1",
    "reselect": "^4.1.8",
    "swr": "^2.3.2"
  }
}
