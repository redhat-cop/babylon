{
  "name": "babylon-catalog-ui",
  "version": "0.0.2",
  "description": "An open source build scaffolding utility for web apps.",
  "repository": "https://github.com/redhat-cop/babylon.git",
  "homepage": "https://github.com/redhat-cop/babylon",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "if test \"$KEEP_ASSETS\" != \"true\" ; then npm run clean ; fi ",
    "dr:surge": "node dr-surge.js",
    "build": "webpack --config webpack.prod.js && npm run dr:surge",
    "start": "sirv dist --cors --single --host --port 8080",
    "start:dev": "webpack serve --color --progress --config webpack.dev.js",
    "test": "jest",
    "eslint": "eslint --ext .tsx,.js ./src/",
    "lint": "npm run eslint",
    "format": "prettier --check --write \"./**/*.{tsx,ts,json,css,js}\"",
    "type-check": "tsc --noEmit",
    "ci-checks": "npm run type-check && npm run lint && npm run test",
    "build:bundle-profile": "webpack --config webpack.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "npm run build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/dompurify": "^3.0.0",
    "@types/js-yaml": "^4.0.5",
    "@types/json2csv": "^5.0.3",
    "@types/lodash.throttle": "^4.1.7",
    "@types/react": "^18.0.32",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-window": "^1.8.5",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^9.1.2",
    "babel-plugin-transform-imports": "^2.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.36.0",
    "eslint-plugin-react": "^7.32.2",
    "file-loader": "^6.2.0",
    "history": "^5.3.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-fetch-mock": "^3.0.3",
    "mini-css-extract-plugin": "^2.7.5",
    "postcss": "^8.4.21",
    "postcss-custom-media": "^8.0.2",
    "postcss-loader": "^7.1.0",
    "prettier": "^2.8.7",
    "prop-types": "^15.6.1",
    "raw-loader": "^4.0.2",
    "rimraf": "^4.4.1",
    "sirv-cli": "^2.0.2",
    "style-loader": "^3.3.2",
    "svg-url-loader": "^8.0.0",
    "terser-webpack-plugin": "^5.3.7",
    "ts-jest": "^29.0.5",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.76.3",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@lexical/react": "^0.9.1",
    "@monaco-editor/react": "^4.4.6",
    "@patternfly/react-core": "4.276.8",
    "@patternfly/react-icons": "4.93.6",
    "@patternfly/react-styles": "4.92.6",
    "@patternfly/react-table": "4.112.39",
    "@reduxjs/toolkit": "^1.9.3",
    "asciidoctor": "^2.2.6",
    "dompurify": "^3.0.1",
    "fuse.js": "^6.6.2",
    "js-yaml": "^4.1.0",
    "json2csv": "^5.0.7",
    "lexical": "^0.9.0",
    "lodash.throttle": "^4.1.1",
    "parse-duration": "^1.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "react-window": "^1.8.8",
    "redux": "^4.2.1",
    "reselect": "^4.1.7",
    "swr": "^2.1.1"
  }
}
