{
  "name": "babylon-catalog-ui",
  "version": "0.0.2",
  "description": "An open source build scaffolding utility for web apps.",
  "repository": "https://github.com/redhat-cop/babylon.git",
  "homepage": "https://github.com/redhat-cop/babylon",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "npm run clean",
    "dr:surge": "node dr-surge.js",
    "build": "webpack --config webpack.prod.js && npm run dr:surge",
    "start": "sirv dist --cors --single --host --port 8080",
    "start:dev": "webpack serve --color --progress --config webpack.dev.js",
    "test": "jest",
    "eslint": "eslint --ext .tsx,.js ./src/",
    "lint": "npm run eslint",
    "format": "prettier --check --write \"./**/*.{tsx,ts,json,css,js}\"",
    "type-check": "tsc --noEmit",
    "ci-checks": "npm run type-check && npm run lint && npm run test",
    "build:bundle-profile": "webpack --config webpack.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "npm run build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.1.1",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-router-dom": "^5.3.0",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "dotenv-webpack": "^7.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "jest-fetch-mock": "^3.0.3",
    "mini-css-extract-plugin": "^2.6.0",
    "prettier": "^2.4.0",
    "prop-types": "^15.6.1",
    "raw-loader": "^4.0.2",
    "rimraf": "^3.0.2",
    "sirv-cli": "^2.0.2",
    "style-loader": "^3.3.1",
    "svg-url-loader": "^7.1.1",
    "terser-webpack-plugin": "^5.3.1",
    "ts-jest": "^28.0.2",
    "ts-loader": "^9.3.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "tslib": "^2.4.0",
    "typescript": "^4.4.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.72.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.4.5",
    "@patternfly/patternfly": "^4.194.4",
    "@patternfly/react-core": "^4.214.1",
    "@patternfly/react-icons": "^4.65.1",
    "@patternfly/react-styles": "^4.64.1",
    "@patternfly/react-table": "^4.83.1",
    "@reduxjs/toolkit": "^1.8.1",
    "asciidoctor": "^2.2.6",
    "classnames": "^2.3.1",
    "dompurify": "^2.3.8",
    "js-yaml": "^3.14.1",
    "parse-duration": "^1.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^8.0.2",
    "react-router-dom": "^5.3.3",
    "react-star-ratings": "^2.3.0",
    "redux": "^4.2.0",
    "reselect": "^4.1.5"
  }
}
